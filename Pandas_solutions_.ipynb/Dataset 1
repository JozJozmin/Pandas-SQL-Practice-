      "cell_type": "code",
      "source": [
        "# 3. binning of deposit column\n",
        "bins = list(np.arange(0,10500,500))\n",
        "df_ft['first_deposit_amounts_binned'] = pd.cut(df_ft['FIRST_DEPOSIT_AMOUNT'], bins)"
